    <LynxUsbDevice name="Control Hub Portal" serialNumber="(embedded)" parentModuleAddress="173">
        <LynxModule name="Control Hub" port="173">
            <Matrix12vMotor name="leftBack" port="3" />
            <Matrix12vMotor name="leftFront" port="2" />
            <Matrix12vMotor name="rightBack" port="1" />
            <Matrix12vMotor name="rightFront" port="0" />
            <CRServo name="intakeServo1" port="0" />
            <CRServo name="intakeServo2" port="1" />
<!--            <CRServo name="intakeServo3" port="2" />-->
        </LynxModule>
        <LynxModule name="Expansion Hub" port="2">
            <Matrix12vMotor name="leftLauncher" port="0" />
            <Matrix12vMotor name="rightLauncher" port="1" />
<!--            <Matrix12vMotor name="catapultWinch" port="2" />-->
            <LynxEmbeddedIMU name="imu" port="0" bus="0"/>
        </LynxModule>
    </LynxUsbDevice>
